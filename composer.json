{
    "name": "inviqa/composer-seeder",
    "description": "A post-create-project-cmd plugin to make source projects templatable",
    "type": "composer-plugin",
    "require": {
        "php": ">=5.6",
        "symfony/finder": "^3.1",
        "twig/twig": "^1.24"
    },
    "autoload": {
      "psr-4": {
        "Inviqa\\Seeder\\": "src/"
      }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Andy Thompson",
            "email": "athompson@inviqa.com"
        }
    ],
    "scripts": {
        "post-create-project-cmd": [
            "Inviqa\\Seeder\\ScriptHandler::postCreateProject",
            "@composer update"
        ]
    }
}
